{
  "extends": "tslint:latest",
  "rules": {
    "semicolon": [
      true,
      "never"
    ],
    "quotemark": [
      false
    ],
    "no-console": [
      false
    ],
    "no-submodule-imports": [
      false
    ],
    "max-line-length": [
      false
    ],
    "no-string-literal": [
      false
    ],
    "prefer-conditional-expression": [
      false
    ],
    "no-var-requires": false
  }
}